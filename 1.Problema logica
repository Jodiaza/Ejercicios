//1. Diseñar un programa en que permita evaluar la expresión (p || q && r).
import java.util.Scanner;

class TestClass {
    public static void main(String args[] ) throws Exception {
        
        Scanner var = new Scanner(System.in);
        
        boolean p = var.nextBoolean();
        boolean q = var.nextBoolean();
        boolean r = var.nextBoolean();
        
        boolean res = (p || q) && r;
        
        System.out.println(res);
        
    }
}
